{
  "manifest_version": 3,
  "name": "Max Autolytics",
  "version": "1.0",
  "description": "",
  "action": {
      "default_popup": "index.html",
      "default_title": "Max Autolytics",
      "default_icon": "logo.png"
  },
  "permissions": [
      "scripting",
      "tabs",
      "activeTab",
      "storage"
  ],
  "host_permissions": [
      "https://*/*",
      "http://*/*"
  ],
  "icons": {
    "16": "logo.png",
    "32": "logo.png"
  }
}